<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.module03_basicgui_db_interface.LoginController" prefHeight="560.0" prefWidth="850.0">
    <children>
        <TextField fx:id="usernameField" layoutX="325.0" layoutY="200.0" promptText="Username" />
        <PasswordField fx:id="passwordField" layoutX="325.0" layoutY="250.0" promptText="Password" />
        <Button layoutX="325.0" layoutY="300.0" mnemonicParsing="false" onAction="#handleLogin" text="Login" />
        <Button layoutX="425.0" layoutY="300.0" mnemonicParsing="false" onAction="#handleRegister" text="Register" />
    </children>
</AnchorPane>
